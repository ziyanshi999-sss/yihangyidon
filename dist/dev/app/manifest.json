{
  "@platforms": [
    "android",
    "iPhone",
    "iPad"
  ],
  "id": "__UNI__ABC_BANK_APP",
  "name": "中国农业银行",
  "version": {
    "name": "1.0.0",
    "code": "100"
  },
  "description": "中国农业银行官方APP，提供全面的金融服务",
  "developer": {
    "name": "",
    "email": "",
    "url": ""
  },
  "permissions": {
    "Fingerprint": {},
    "FaceID": {},
    "Payment": {},
    "Push": {},
    "Share": {},
    "OAuth": {},
    "Barcode": {},
    "Camera": {},
    "Contacts": {},
    "Geolocation": {},
    "Maps": {
      "coordType": "gcj02"
    },
    "Messaging": {},
    "Speech": {},
    "UniNView": {
      "description": "UniNView原生渲染"
    }
  },
  "plus": {
    "useragent": {
      "value": "uni-app",
      "concatenate": true
    },
    "splashscreen": {
      "target": "id:1",
      "autoclose": true,
      "waiting": true,
      "delay": 0
    },
    "popGesture": "close",
    "launchwebview": {
      "id": "1",
      "kernel": "WKWebview"
    },
    "usingComponents": true,
    "nvueStyleCompiler": "uni-app",
    "compilerVersion": 3,
    "distribute": {
      "google": {
        "permissions": [
          "<uses-permission android:name=\"android.permission.CHANGE_NETWORK_STATE\"/>",
          "<uses-permission android:name=\"android.permission.MOUNT_UNMOUNT_FILESYSTEMS\"/>",
          "<uses-permission android:name=\"android.permission.VIBRATE\"/>",
          "<uses-permission android:name=\"android.permission.READ_LOGS\"/>",
          "<uses-permission android:name=\"android.permission.ACCESS_WIFI_STATE\"/>",
          "<uses-feature android:name=\"android.hardware.camera.autofocus\"/>",
          "<uses-permission android:name=\"android.permission.ACCESS_NETWORK_STATE\"/>",
          "<uses-permission android:name=\"android.permission.CAMERA\"/>",
          "<uses-permission android:name=\"android.permission.GET_ACCOUNTS\"/>",
          "<uses-permission android:name=\"android.permission.READ_PHONE_STATE\"/>",
          "<uses-permission android:name=\"android.permission.CHANGE_WIFI_STATE\"/>",
          "<uses-permission android:name=\"android.permission.WAKE_LOCK\"/>",
          "<uses-permission android:name=\"android.permission.FLASHLIGHT\"/>",
          "<uses-feature android:name=\"android.hardware.camera\"/>",
          "<uses-permission android:name=\"android.permission.WRITE_SETTINGS\"/>"
        ]
      },
      "apple": {
        "privacyDescription": {
          "NSCameraUsageDescription": "农业银行需要使用相机进行扫码支付和身份认证",
          "NSPhotoLibraryUsageDescription": "农业银行需要访问相册来上传身份证照片",
          "NSLocationWhenInUseUsageDescription": "农业银行需要获取位置信息用于网点查询和风险控制",
          "NSContactsUsageDescription": "农业银行需要访问通讯录来快速转账给联系人",
          "NSMicrophoneUsageDescription": "农业银行需要使用麦克风进行语音客服",
          "NSFaceIDUsageDescription": "农业银行需要使用Face ID进行安全验证"
        },
        "capabilities": {
          "entitlements": {
            "com.apple.developer.associated-domains": [
              "applinks:m.abchina.com"
            ]
          }
        }
      },
      "plugins": {
        "payment": {
          "alipay": {
            "scheme": "abcbank"
          },
          "weixin": {
            "appid": "wx1234567890abcdef"
          }
        },
        "oauth": {
          "weixin": {
            "appid": "wx1234567890abcdef",
            "appsecret": "your_wechat_secret"
          },
          "qq": {
            "appid": "123456789"
          }
        },
        "push": {
          "unipush": {
            "enable": true
          }
        },
        "maps": {
          "amap": {
            "key": "your_amap_key"
          },
          "baidu": {
            "key": "your_baidu_key"
          }
        },
        "audio": {
          "mp3": {
            "description": "Android平台录音支持MP3格式文件"
          }
        }
      }
    },
    "statusbar": {
      "immersed": "supportedDevice",
      "style": "dark",
      "background": "#F8F8F8"
    },
    "uniStatistics": {
      "enable": false
    },
    "allowsInlineMediaPlayback": true,
    "safearea": {
      "background": "#ffffff",
      "bottom": {
        "offset": "auto"
      }
    },
    "uni-app": {
      "control": "uni-v3",
      "vueVersion": "3",
      "compilerVersion": "4.75",
      "nvueCompiler": "uni-app",
      "renderer": "auto",
      "nvue": {
        "flex-direction": "column"
      },
      "nvueLaunchMode": "normal",
      "webView": {
        "minUserAgentVersion": "49.0"
      }
    },
    "tabBar": {
      "position": "bottom",
      "color": "#7A7E83",
      "selectedColor": "#3cc51f",
      "borderStyle": "rgba(0,0,0,0.4)",
      "blurEffect": "none",
      "fontSize": "10px",
      "iconWidth": "24px",
      "spacing": "3px",
      "height": "50px",
      "list": [
        {
          "pagePath": "pages/index/index",
          "iconPath": "/static/tabbar/home.png",
          "selectedIconPath": "/static/tabbar/home-active.png",
          "text": "首页"
        },
        {
          "pagePath": "pages/wealth/wealth",
          "iconPath": "/static/tabbar/service.png",
          "selectedIconPath": "/static/tabbar/service-active.png",
          "text": "财富"
        },
        {
          "pagePath": "pages/life/life",
          "iconPath": "/static/tabbar/order.png",
          "selectedIconPath": "/static/tabbar/order-active.png",
          "text": "生活"
        },
        {
          "pagePath": "pages/user/user",
          "iconPath": "/static/tabbar/user.png",
          "selectedIconPath": "/static/tabbar/user-active.png",
          "text": "我的"
        }
      ],
      "backgroundColor": "#ffffff",
      "selectedIndex": 0,
      "shown": true,
      "child": [
        "lauchwebview"
      ],
      "selected": 0
    }
  },
  "app-harmony": {
    "useragent": {
      "value": "uni-app",
      "concatenate": true
    },
    "uniStatistics": {
      "enable": false
    },
    "safearea": {
      "background": "#ffffff",
      "bottom": {
        "offset": "auto"
      }
    }
  },
  "launch_path": "__uniappview.html"
}